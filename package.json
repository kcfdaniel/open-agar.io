{
  "name": "open-agar.io",
  "version": "1.0.0",
  "description": "A simple Agar.io clone",
  "type": "module",
  "scripts": {
    "build": "npm run build:client && npm run build:server && npm run test",
    "build:client": "tsc && vite build",
    "build:server": "tsc",
    "dev:client": "vite",
    "dev:server": "tsx watch server/src/server.ts",
    "preview": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "todo": "leasot 'src/**/*.ts' > TODO.md"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Daniel Kwok",
  "maintainers": [],
  "license": "MIT",
  "contributors": [],
  "dependencies": {
    "express": "^4.18.2",
    "leasot": "^13.3.0",
    "nodemon": "^3.0.1",
    "sat": "^0.9.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/sat": "^0.0.35",
    "@types/uuid": "^10.0.0",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.5.0",
